{
    "compilerOptions": {
        "declaration": false,
        "strict": false,
        "moduleResolution": "Node",
        "module": "ESNext",
        "target": "ESNext",
        "jsx": "react-jsx",
        "baseUrl": ".",
        "rootDir": ".",
        "lib": [
            "ESNext",
            "DOM"
        ],
        "paths": {},
        "esModuleInterop": true,
        "skipLibCheck": true,
    },
    "typeRoots": [
        "node_modules/@types",
        "types"
    ],
    "include": [
        "src/**/*",
        "types"
    ],
    "exclude": [
        "node_modules"
    ]
}